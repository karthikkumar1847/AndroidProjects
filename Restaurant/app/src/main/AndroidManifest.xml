<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pradeep.restaurant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/foody"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginRegActivity" />
        <activity android:name=".Login" />
        <activity android:name=".Register" />
        <activity android:name=".AllRes" />
        <activity android:name=".Account" />
        <activity android:name=".RMenu" />
        <activity android:name=".MenuIndian" />
        <activity android:name=".MenuAmerican" />
        <activity android:name=".MenuMexican" />
        <activity android:name=".MenuChinese" />
        <activity android:name=".Cart" />
        <activity android:name=".OrderHistory" />
        <activity android:name=".OrderDetails"></activity>
    </application>

</manifest>